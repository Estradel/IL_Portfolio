{
  "name": "blowfish_template",
  "version": "1.0.0",
  "description": "Blowfish Template",
  "main": "index.js",
  "scripts": {
    "server": "hugo server -b http://localhost -p 8000",
    "dev": "NODE_ENV=development npm install --save-dev tailwindcss./themes/blowfish/node_modules/tailwindcss/lib/cli.js -c ./themes/blowfish/tailwind.config.js -i ./themes/blowfish/assets/css/main.css -o ./assets/css/compiled/main.css --jit -w",
    "build": "cross-env NODE_ENV=production \"./themes/blowfish/node_modules/tailwindcss/lib/cli.js\" -c ./themes/blowfish/tailwind.config.js -i ./themes/blowfish/assets/css/main.css -o ./assets/css/compiled/main.css --jit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nunocoracao/blowfish_template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nunocoracao/blowfish_template/issues"
  },
  "homepage": "https://github.com/nunocoracao/blowfish_template#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "tailwindcss": "^4.1.4"
  }
}
